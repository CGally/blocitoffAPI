<div class="jumbotron">
  <h1>Blocitoff</h1>
  <p>A resource for managing to-do lists!</p>
</div>
<h2>Users</h2>
<div class="col-md-8">
<% @users.each do |user| %>
  <% if !user.private || current_user == user %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= link_to user.username, user %>
        </h4>
      </div>
    </div>
  <% end %>
<% end %>
</div>
